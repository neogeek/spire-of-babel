{
  "name": "spire-of-babel",
  "description": "A one stop solution for working with ES6, React and TypeScript.",
  "version": "2.0.1",
  "bin": {
    "spire-of-babel": "./bin/spire-of-babel.js"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=10.0"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-flow": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "babelify": "10.0.0",
    "browserify": "16.5.2",
    "chalk": "4.1.0",
    "findup-sync": "4.0.0",
    "parse-cmd-args": "3.0.0",
    "update-notifier": "4.1.1"
  },
  "devDependencies": {
    "jest": "26.4.2"
  },
  "scripts": {
    "test": "jest test/specs/",
    "coverage": "jest test/specs/ --coverage",
    "fixtures": "test/transform.sh"
  },
  "jest": {
    "testRegex": "./test/specs/.+\\.js$"
  },
  "keywords": [
    "es6",
    "es2015",
    "babel",
    "browserify",
    "react",
    "eslint",
    "typescript"
  ],
  "authors": [
    {
      "name": "Scott Doxey",
      "email": "hello@scottdoxey.com",
      "homepage": "http://scottdoxey.com/"
    }
  ],
  "homepage": "https://github.com/neogeek/spire-of-babel",
  "repository": {
    "type": "git",
    "url": "git://github.com/neogeek/spire-of-babel.git"
  }
}
